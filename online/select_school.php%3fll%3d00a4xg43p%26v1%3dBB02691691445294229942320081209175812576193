            <script type="text/javascript">
            	function popup(el, opts) {
            	  if (!el) {
            	    return true;
            	  }
            	  var target = '_blank', url = el.href;
            	  if (!opts) {
            	    opts = '';
            	  }
            	  if (el.target !== '') {
            	    target = el.target;
            	  }
            	  window.open(url, target, opts);
            	  return false;
            	}
            </script>
            
