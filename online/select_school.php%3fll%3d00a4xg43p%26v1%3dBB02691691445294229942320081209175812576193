		function submitForm(e)
		{
			var keynum;
			if(window.event) // IE
			{
				keynum = e.keyCode;
			}
			else if(e.which) // Netscape/Firefox/Opera
			{
				keynum = e.which;
			}
			if (keynum == 13)
				StateIDForm.submit();
		}
	
	</script>				
				

	
				

				


			  
